{
  "name": "thought",
  "version": "1.5.2",
  "description": "A customizable documentation generator for github projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/nknapp/thought.git"
  },
  "author": {
    "name": "Nils Knappmeier",
    "email": "npm@knappi.org",
    "url": "https://github.com/nknapp"
  },
  "bugs": {
    "url": "https://github.com/nknapp/thought/issues"
  },
  "main": "index.js",
  "bin": {
    "thought": "bin/thought.js"
  },
  "preferGlobal": true,
  "files": [
    "index.js",
    "customize.js",
    "lib",
    "handlebars",
    "bin"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "eslint --fix .",
    "test": "mocha && npm run lint",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha",
    "postcoverage": "istanbul check-coverage coverage/coverage.json --statements 100",
    "thought": "bin/thought.js run -a",
    "postthought": "node examples/run-thought.js",
    "version": "thoughtful changelog -o -a && npm run thought"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "bluebird": "^3.5.0",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.11.0",
    "customize": "^2.0.1",
    "customize-engine-handlebars": "^2.2.2",
    "customize-write-files": "^2.0.1",
    "debug": "^3.0.1",
    "deep-aplus": "^1.0.4",
    "glob": "^7.1.2",
    "handlebars": "^4.0.10",
    "lodash.clonedeep": "^4.5.0",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.groupby": "^4.6.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.map": "^4.6.0",
    "m-io": "^0.5.0",
    "popsicle": "^10.0.0",
    "semver": "^5.4.1",
    "simple-git": "^1.75.0",
    "trace-and-clarify-if-possible": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.0",
    "nock": "^9.0.14",
    "recursive-copy": "^2.0.6",
    "thought-plugin-jsdoc": "^1.0.1",
    "thoughtful-release": "^0.3.1"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**"
    ]
  },
  "keywords": []
}
